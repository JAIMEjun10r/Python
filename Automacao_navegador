pyautogui.PAUSE = 1
pyautogui.press('winleft')
pyautogui.write('Chrome')
pyautogui.press('enter')
sleep(1)
pyautogui.write('https://google.com')
pyautogui.press('enter')
sleep(5)
pyautogui.moveTo(271, 17)
pyautogui.click()
pyautogui.write('gmail.com')
sleep(1)
pyautogui.press('enter')
sleep(0.8)
#Aqui o ponteiro do mouse se dirige ao bot√£o de + na lateral do google, para adicionar uma nova aba
pyautogui.moveTo(332, 16)
sleep(1)
pyautogui.mouseDown()
#serve para clicar e manter pressionado
sleep(0.5)
pyautogui.moveTo(303, 66)
pyautogui.mouseUp()
sleep(1)
pyautogui.hotkey('alt','tab')
sleep(0.5)
